    "compile": "yarn prettier:solidity && yarn hardhat compile",
    "test": "yarn lint && yarn hardhat test",
    "deploy": "yarn eslint:all-fix && yarn hardhat deploy",
    "verify": "yarn hardhat etherscan-verify",
    "node": "yarn hardhat node",
    "eslint:all": "yarn eslint deploy/**/*.js deploy/**/*.deploy.js test/**/*.js test/**/*.test.js test/*.test.js tasks/*.js tasks/**/*.js tasks/**/*.deploy.js tasks/**/**/*.js tasks/**/**/**/*.js config/*.js scripts/*.js",
    "eslint:all-fix": "yarn eslint --fix deploy/**/*.js deploy/**/*.deploy.js test/**/*.js test/**/*.test.js test/*.test.js tasks/*.js tasks/**/*.js tasks/**/*.deploy.js tasks/**/**/*.js tasks/**/**/**/*.js config/*.js scripts/*.js",
    "solhint:all": "yarn solhint -c .solhint.json contracts/*.sol contracts/**/*.sol contracts/**/**/*.sol",
    "prettier:solidity": "./node_modules/.bin/prettier --write contracts/*.sol contracts/**/*.sol contracts/**/**/*.sol",
    "prettier:js": "./node_modules/.bin/prettier --write deploy/**/*.js deploy/**/*.deploy.js test/**/*.js test/**/*.test.js test/*.test.js tasks/*.js tasks/**/*.js tasks/**/*.deploy.js tasks/**/**/*.js tasks/**/**/**/*.js config config/*/* scripts",
    "prepare": "cd .. && husky install src/.husky",
    "postinstall": "patch-package",
    "lint": "yarn prettier:solidity && yarn prettier:js && yarn eslint:all-fix && yarn solhint:all",
    "coverage": "export COVERAGE=true; export NODE_OPTIONS='--max-old-space-size=4096'; yarn hardhat coverage"
  },
